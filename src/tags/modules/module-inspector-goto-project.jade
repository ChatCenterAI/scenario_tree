  style(scoped, type='less').
    :scope {
      width: 100%;
      height: 100%;
      .wrap-module {
        .title {
          color: #2196F3;
          font-weight: 600;
          font-size: 16px;
        }
        .wrap-project {
          p {
            display: inline-block;
          }
          a {
            color: #2196F3;
            border-bottom: solid #2196F3 1.5px;
            cursor: pointer;
          }
        }
      }
    }


  script.

    var self = this;
    this.content = opts.__proto__.content;
    
    this.text = `${this.content.text}`;
    this.href = `./#projects/${this.content.scenarioId}`;